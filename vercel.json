{
    "version": 2,
    "builds": [
        {
            "src": "/client/dist/$1",
            "use": "@vercel/node"
        },
        {
            "src": "/server/dist/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/dist/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/client/dist/$1"
        }

    ]
}