{
    "version": 2,
    "builds": [
        {
            "src": "/client/package.json",
            "use": "@vercel/static-build",
            "config": {"buildCommand": "npm run build"}
        },
        {
            "src": "/server/dist/app.js",
            "use": "@vercel/static-build",
            "config": {"buildCommand": "npm run build"}
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/dist/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/client/dist/$1"
        }

    ]
}