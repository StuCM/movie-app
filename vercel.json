{
    "version": 2,
    "builds": [
        {
            "src": "./server/dist/app.js",
            "use": "@vercel/node"
        },
        {
            "src": "./client/package.json",
            "use": "@vercel/static-build",
            "config": {"distDir": "client/"}
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/dist/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/client/dist/index.html"
        }

    ]
}